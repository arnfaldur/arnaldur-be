{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "esnext",
        "module": "esnext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsxImportSource": "solid-js",
        "jsx": "preserve",
        "strict": true,
        "types": ["solid-start/env", "vite/client"],
        "baseUrl": "./",
        "paths": {
            "~/*": ["./src/*"]
        }
    }
}
